{
  "expo": {
    "name": "fintech-ui",
    "slug": "fintech-ui",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "exp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Pick a receipt photo to parse",
        "NSCameraUsageDescription": "Scan receipts with the camera",
        "NSFaceIDUsageDescription": "Use Face ID to quickly and securely sign you into your account",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.ivampyy.fintechui"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "permissions": [
        "READ_MEDIA_IMAGES",
        "CAMERA"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ivampyy.fintechui"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bea271af-0a9f-42a2-990f-efdda9de22fd"
      }
    }
  }
}
